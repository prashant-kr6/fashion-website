function init()
{
	setTimeout("loadtext()",3000);	
}
function loadtext() 
{
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
      document.getElementById("first").innerHTML =
      this.responseText;
    }
  };
  xhttp.open("GET", "txt/text.txt", true);
  setTimeout("loadimages()",3000);
  xhttp.send();
}
function loadimages() 
{
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
      document.getElementById("sec").src =
      this.responseText;
    }
  };
  xhttp.open("GET", "txt/img2.txt", true);
 setTimeout(loadimages1(),6000);
  xhttp.send();
}

